    "can-component": "^3.0.3",
    "can-list": "^3.0.1",
    "can-map": "^3.0.3",
    "can-map-define": "^3.0.2",
    "can-route": "^3.0.5",
    "funcunit": "^3.1.0",
    "lodash.template": "^4.4.0",
    "steal": "^1.0.3",
    "steal-qunit": "1.0.1",
    "steal-tools": "^1.9.0",
    "testee": "^0.7.0"
  },
  "homepage": "https://github.com/donejs/autorender",
